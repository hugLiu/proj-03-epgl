
@{
    ViewBag.Title = "新疆油气田开发规划图库管理平台";
    Layout = "~/Views/Shared/_EPGLLayout.cshtml";
}
<div class="col-md-3 col-sm-3" style="padding-right: 5px; padding-left: 5px;">
    <div class="ibox float-e-margins">
        <div class="ibox-content" style="padding: 5px 5px 5px 5px;">
            <div class="form-group">
                <label>图名： </label>
                <div class="col-sm-12">
                    <div class="row well" style="padding: 5px; margin-bottom: 0px;">
                        <div class="col-md-9" style="padding-left: 0px;">
                            <input id="txtGdbTitle" type="text" class="form-control" style="padding: 5px 5px;" value="图名自动生成">
                        </div>
                        <div class="col-md-3" style="padding-left: 5px;">
                            <button id="btnSelTraget" class="btn btn-sm btn-warning btn-outline" type="button">确定工区</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="form-group" style="margin-bottom: 2px;">
                <label>图层图元： </label>
                <div class="row">
                    <div class="col-md-12 col-sm-12" style="padding-right: 1px; ">
                        <div class="jqGrid_wrapper">
                            <table id="table_new_layers"></table>
                            <div id="pager_new_layers"></div>
                        </div>
                    </div>
                    <!--
                                                        <div class="col-md-4 col-sm-4" style="padding-left: 1px;">
                                                            <div class="jqGrid_wrapper">
                                                                <table id="table_new_items"></table>
                                                                <div id="pager_new_items"></div>
                                                            </div>
                                                        </div>-->
                </div>
            </div>
        </div>
    </div>
</div>
<div class="col-md-9 col-sm-9">
    <div class="row">
        <div class="col-md-11 col-sm-11" style="width:94%; padding-right: 5px; padding-left: 5px;">
            <div class="ibox float-e-margins" style="margin-bottom: 0px;">
                <div id="panelWork" class="ibox-content" style="padding: 5px 5px 0px 5px;">
                    <OBJECT id="joGIS1" classid="clsid:D2546426-13FD-4018-85F1-F5BD61147C8F" CODEBASE="JoGIS4.ocx#version=1,0,0,1" style="width:100%;height:524px" wmode="Opaque"
                            VIEWASTEXT>
                        <PARAM NAME="_Version" VALUE="65536">
                        <PARAM NAME="_ExtentX" VALUE="20000">
                        <PARAM NAME="_ExtentY" VALUE="15000">
                        <PARAM NAME="_StockProps" VALUE="0">
                        <PARAM NAME="WMODE" VALUE="transparent">
                    </OBJECT>
                </div>
            </div>
        </div>
        <div class="col-md-1 col-sm-1" style="width: 6%; padding-right: 5px; padding-left: 5px;">
            <div class="ibox float-e-margins" style="margin-bottom: 5px;">
                <div id="panelBtns" class="ibox-content" style="padding: 5px 5px 0px 5px;">
                    <button class="btn btn-success  dim" type="button" title="选取" style="width:38px"><i class="fa fa-mouse-pointer"></i></button>
                    <button class="btn btn-info  dim" type="button" title="放大"><i class="fa fa-expand"></i></button>
                    <button class="btn btn-warning  dim" type="button" title="缩小"><i class="fa fa-compress"></i></button>
                    <button class="btn btn-default  dim " type="button" title="移动"><i class="fa fa-hand-paper-o"></i></button>
                    <button class="btn btn-outline btn-warning dim " type="button" title="复位"><i class="fa fa-circle-o-notch"></i></button>
                    <button class="btn btn-danger  dim " type="button" title="截取"><i class="fa fa-circle-o-notch"></i></button>
                </div>
            </div>
        </div>
    </div>

</div>

@section script{
<script src="~/DemoData/json/data-tragets.js"></script>
<script src="~/DemoData/json/data-tragets-layertypes.js"></script>
<script src="~/DemoData/json/data-thumb.js"></script>
<script src="~/Content/EPGL/js/pages/flexdraw/index.js"></script>
    }

