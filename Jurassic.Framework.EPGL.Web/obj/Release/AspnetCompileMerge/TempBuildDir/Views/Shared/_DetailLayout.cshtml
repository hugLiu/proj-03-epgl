
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    @*<meta charset="utf-8" />*@
    <meta http-equiv="pragma" content="no-cache">
    <meta http-equiv="Cache-Control" content="no-cache, must-revalidate">
    <meta http-equiv='X-UA-Compatible' content='IE=edge'>
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1">
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta name="keywords" content="">
    <meta name="description" content="">
    <link rel="shortcut icon" href="favicon.ico">
    <link href="~/Content/EPGL/css/core/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/EPGL/css/core/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/EPGL/css/core/font-awesome-ie7.min.css" rel="stylesheet" />
    <link href="~/Content/EPGL/css/core/animate.css" rel="stylesheet" />

    <!--kendo-->
    <link href="~/Content/EPGL/css/plugins/kendo/kendo.common-bootstrap.min.css" rel="stylesheet" />

    <link href="~/Content/EPGL/css/shared/style.css" rel="stylesheet" />
    <link href="~/Content/EPGL/css/shared/layout.css" rel="stylesheet" />
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="~/Content/EPGL/js/shared/html5shiv/3.7/html5shiv.min.js"></script>
    <script src="~/Content/EPGL/js/shared/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    <style type="text/css">
        .inline-pane {
            display: inline-block;
            vertical-align: middle;
        }
        .left-pane {
            width: calc(100%-110px);
            width: -moz-calc(100% - 110px);
            width: -webkit-calc(100% - 110px);
        }
        .right-pane {
            width: 110px;
            height: 100%;
        }
    </style>
</head>
<body class="gray-bg pace-done">
<div class="wrapper wrapper-content animated fadeInRight" style="padding: 5px 5px;" id="verticalSplitter">
    <div id="top-pane">
        <div class="row">
            <div class="col-md-9 col-sm-9">
                <div class="inline-pane left-pane">
                    <nav aria-label="switchPicture">
                        <ul class="pager">
                            <li class="previous"><a><i class="fa fa-caret-left fa-lg" aria-hidden="true"></i></a></li>
                            <li><span></span></li>
                            <li class="next"><a><i class="fa fa-caret-right fa-lg" aria-hidden="true"></i></a></li>
                        </ul>
                    </nav>
                    <OBJECT id="joGIS1" classid="clsid:D2546426-13FD-4018-85F1-F5BD61147C8F" CODEBASE="JoGIS4.ocx#version=1,0,0,1" style="width: 100%; height: 524px" wmode="Opaque"
                            VIEWASTEXT>
                        <PARAM NAME="_Version" VALUE="65536">
                        <PARAM NAME="_ExtentX" VALUE="20000">
                        <PARAM NAME="_ExtentY" VALUE="15000">
                        <PARAM NAME="_StockProps" VALUE="0">
                        <PARAM NAME="WMODE" VALUE="transparent">
                    </OBJECT>
                </div>
                <div class="inline-pane right-pane">
                    <ul class="list-group">
                        <li class="list-group-item"><a href="#" id="tool_select">选取</a></li>
                        <li class="list-group-item"><a href="#" id="tool_zoomin">放大</a></li>
                        <li class="list-group-item"><a href="#" id="tool_zoomout">缩小</a></li>
                        <li class="list-group-item"><a href="#" id="tool_move">移动</a></li>
                        <li class="list-group-item"><a href="#" id="tool_reset">复位</a></li>
                        <li class="list-group-item"><a href="#" id="tool_mapdata">图数</a></li>
                        <li class="list-group-item"><a href="#" id="tool_status">现状</a></li>
                        <li class="list-group-item"><a href="#" id="tool_curve">曲线</a></li>
                        <li class="list-group-item"><a href="#" id="tool_download">下载</a></li>
                        <li class="list-group-item"><a href="#" id="tool_update">更新</a></li>
                    </ul>
                </div>

            </div>

            <div class="col-md-3 col-sm-3">
                <div class="ibox float-e-margins" style="margin-bottom: 0px;">
                    <!--<div class="ibox-title">
                    <h5>图件信息 <small>xxxxx</small></h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                    </div>
                </div>-->
                    <div class="ibox-content" style="padding: 15px 15px 0px 15px;">
                        <h4 class="font-bold">图名：<small id="gdbtitle">333</small></h4>
                        <h4 class="font-bold">编图人：<small>徐献高、沈金龙</small></h4>
                        <h4 class="font-bold">绘图人：<small>沈金龙、李艳九</small></h4>
                        <h4 class="font-bold">审核人：<small>斯春松</small></h4>
                        <h4 class="font-bold">制图日期：<small>2015-11-30 13:33:22</small></h4>
                        <p>
                            <button class="btn btn-outline btn-success  dim" style="width: 60px; height: 30px; font-size: 12px;" type="button"><i class="fa fa-download"></i>下载</button>
                            <button class="btn btn-outline btn-danger  dim " style="width: 60px; height: 30px; font-size: 12px;" type="button"><i class="fa fa-heart"></i>收藏</button>
                            <button class="btn btn-outline btn-warning dim" style="width: 60px; height: 30px; font-size: 12px;" type="button"><i class="fa fa-print"></i>打印</button>
                            <button class="btn btn-primary dim" style="width: 60px; height: 30px; font-size: 12px;" type="button" onclick='window.open("/Viewer/Updatedetails", "_blank");'><i class="fa fa-warning"></i>更新</button>
                        </p>
                    </div>
                </div>

                <div class="ibox float-e-margins" style="margin-bottom: 0px;">
                    <div class="ibox-title">
                        <h5>图层 <small>控制\查看</small></h5>
                        <div class="ibox-tools">
                            <a class="collapse-link">
                                <i class="fa fa-chevron-up"></i>
                            </a>
                        </div>
                    </div>
                    <div class="ibox-content" style="padding: 5px 5px 0px 5px;">
                        <div id="jsonTree_GeoLayers"></div>
                    </div>
                </div>
            </div><!-- col end -->
        </div>
    </div>
    <div id="bottom-pane">
        <div class="row">
            <div class="col-md-12 col-sm-12">
                <ul class="nav nav-tabs" role="tablist">
                    <li role="presentation" class="active"><a href="#drilling" aria-controls="drilling" role="tab">钻井</a></li>
                    <li role="presentation"><a href="#capacity" aria-controls="capacity" role="tab">产能</a></li>
                    <li role="presentation"><a href="#reserves" aria-controls="reserves" role="tab">储量</a></li>
                </ul>
                <div class="tab-content">
                    <div role="tabpanel" class="tab-pane active" id="drilling">
                        <div id="drilling_table"></div>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="capacity">
                        <table id="capacity_table"></table>
                    </div>
                    <div role="tabpanel" class="tab-pane" id="reserves">
                        <table id="reserves_table"></table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
</div>
    <!-- 全局js -->
    <script src="~/Content/EPGL/js/core/jquery-2.1.4.min.js"></script>
    <script src="~/Content/EPGL/js/core/knockout-3.0.0.js"></script>
<script src="~/Content/EPGL/js/core/bootstrap.js"></script>

    <script src="~/Content/EPGL/js/shared/jogis.js_v4.0/jogis-frame.js"></script>
    <!-- jsTree plugin javascript -->
<script src="~/Content/EPGL/js/plugins/jsTree/jstree.min.js"></script>
<!--kendo-->
    <script src="~/Content/EPGL/js/plugins/kendo/kendo.all.min.js"></script>
    <!-- 自定义js -->
    <script src="~/Content/EPGL/js/shared/global.js"></script>
<script src="~/Content/EPGL/js/shared/content.js"></script>
    <script src="~/Content/EPGL/js/shared/jsTools.js"></script>
    <!--页面级脚本-->
    @RenderSection("script", false)

</body>
</html>

