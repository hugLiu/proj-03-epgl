                    </div>
                    <div class="col-md-11">
                        <div class="input-group">
                            <input type="text" placeholder="请输入成果名称" class="input-sm form-control" id="myLookTable"> <span class="input-group-btn">
                                <button type="button" class="btn btn-sm btn-primary"> 搜索</button>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="project-list">
                    <table id="table_list1" class="footable table table-stripped toggle-arrow-tiny" data-page-size="10" data-filter="#myLookTable">
                        <thead>
                            <tr>
                                <th style="width:40px">编号</th>
                                <th>名称</th>
                                <th style="width:85px">编图人</th>
                                <th style="width:85px">图件分类</th>
                                <th style="width:100px">制图日期</th>
                                <th style="width:100px">入库日期</th>
                                <th style="width:100px">浏览日期</th>
                                <th style="width:80px">浏览次数</th>
                                <th style="width:160px">操作</th>
                            </tr>
                        </thead>
                        <tbody data-bind="foreach:myLookList">
                            <tr>
                                <td><span class="ibx-hotTop-rank current" data-bind="text:$index() + 1"></span></td>
                                <td><span data-bind="text: METATITLE"></span></td>
                                <td data-bind="text: METAAUTHOR"></td>
                                <td data-bind="text:METAPT"></td>
                                <td data-bind="text:$parent.dateFormat(METACREATEDATE)"></td>
                                <td data-bind="text:$parent.dateFormat(METAINDEXDATE)"></td>
                                <td data-bind="text:$parent.dateFormat(LOOKDATE())"></td>
                                <td class="text-navy"><i class="fa fa-level-up" data-bind="text:LOOKCOUNT"></i></td>
                                <td>
                                    <button class="btn btn-info btn-circle" type="button" data-bind="click: function(data,event){return $parent.openDetailPage(data,event,1);} " data-toggle="tooltip" data-placement="left" title="浏览">
                                        <i class="fa fa-sticky-note-o"></i>
                                    </button>
                                    <!--ko if:FAVORITEFLAG()==1-->
                                    <button class="btn btn-danger btn-circle btn-outline" type="button" data-bind="click: $parent.cancleFavorite" data-toggle="tooltip" data-placement="left" title="取消收藏">
                                        <i class="fa fa-heart"></i>
                                    </button>
                                    <button class="btn btn-warning btn-circle" type="button" data-toggle="modal" data-target="#favCatalog_modal" data-bind="attr:{'data-iiid':IIID,'data-favcatalogid':FAVCATALOGID,'data-favoriteflag':FAVORITEFLAG}" data-tabindex="1" data-placement="left" title="移动">
                                        <i class="fa fa-share"></i>
                                    </button>
                                    <!--/ko-->
                                    <!--ko if:FAVORITEFLAG()==0-->
                                    <button class="btn btn-default btn-circle btn-outline" type="button" data-toggle="modal" data-target="#favCatalog_modal" data-bind="attr:{'data-iiid':IIID,'data-favcatalogid':FAVCATALOGID}" data-tabindex="1" data-placement="left" title="收藏">
                                        <i class="fa fa-heart"></i>
                                    </button>
                                    <!--/ko-->
                                    <button class="btn btn-success btn-circle" type="button" data-bind="click: $parent.downloadFile" data-toggle="tooltip" data-placement="left" title="下载">
                                        <i class="fa fa-download"></i>
                                    </button>
