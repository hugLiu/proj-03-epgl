@{
    ViewBag.Title = "新疆油气田开发规划图库管理平台";
    Layout = "~/Views/Shared/_EPGLLayout.cshtml";
}

@section css{
    <!--footable-->
    <link href="~/Content/EPGL/css/plugins/footable/footable.core.css" rel="stylesheet" />
    <!--jsTree-->
    <link href="~/Content/EPGL/css/plugins/jsTree/style.min.css" rel="stylesheet" />
    <!--bootstrap colorpicker-->
    <link href="~/Content/EPGL/css/plugins/bootstrap-colorpicker/bootstrap-colorpicker.min.css" rel="stylesheet" />
    <!--fileinput-->
    <link href="~/Content/EPGL/css/plugins/fileinput/fileinput.css" rel="stylesheet" />

    <link href="~/Content/EPGL/css/plugins/bootstrap-datetimepicker/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <style>
        .lightBoxGallery img {
            margin: 5px;
            width: 160px;
        }

        .popover {
            z-index: 2050;
        }

        .p-first div {
            margin-bottom: 10px;
        }

        .p-last div {
            margin-top: 20px;
        }

        #collapseTheme .form-group:hover {
            background-color: #a8e4ff;
        }

        .navbar-static-side {
            background-color: #E0E0E0;
        }

        .checkbox-addon {
            width: 209px;
            text-align: left;
        }

        .ts-tab-box {
            display: none;
        }

        .form-label {
            width: 114px;
            text-align: right;
        }
    </style>
}

<form id="resultForm" name="resultform" action="/Viewer/Detail" method="post" target="_blank" style="display: none;">
    <input type="hidden" id="mapId" name="iiid" size="25">
    <input type="hidden" id="mapData" name="data" size="25">
</form>
<form id="formDown" method="post" action="/FileInfo/DownLoad" style="display:none;">
    <input type="hidden" name="id" id="id" />
    <input type="hidden" name="title" id="title" />
    <input type="hidden" name="url" id="url" />
    <input type="hidden" name="ticket" id="ticket" />
    <input type="hidden" name="format" id="format" />
    <input type="hidden" name="name" id="name" />
</form>
<div class="col-sm-3">
    <div class="ibox float-e-margins" style="margin-bottom:5px">
        <div class="ibox-title">
            <h5>个人资料</h5>
        </div>
        <div>
            <div class="ibox-content no-padding border-left-right">
                <img alt="image" class="img-responsive" src="~/Content/EPGL/img/shared/avatar/profile_big.jpg">
            </div>
            <div class="ibox-content profile-content">
                <h4><strong>@ViewBag.userName</strong></h4>
                <p><i class="fa fa-map-marker"></i>新疆油田勘探开发研究院战略规划研究所</p>
                <div class="user-button">
                    <div class="row">
                        <div class="col-sm-6">
                            @*<button type="button" class="btn btn-primary btn-sm btn-block"><i class="fa fa-upload"></i> 提交成果</button>*@
                        </div>
                        <div class="col-sm-6">
                            <a class="btn btn-warning btn-sm btn-block" data-load-url="@Url.Action("changepassword", "account")" data-toggle="modal" data-target="#changePassword_modal"><i class="fa fa-coffee"></i> 修改密码</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--左侧导航开始-->
    <div id="wrapper" style="margin-top:0px">
        <nav class="navbar-default navbar-static-side" role="navigation" style="height:600px">
            <!--<div class="nav-close"><i class="fa fa-times-circle"></i>
            </div>-->
            <div class="sidebar-collapse">
                <ul class="nav" id="side-menu">
                    <li>
                        <a href="javascript:switchtab(1)">
                            <i class="fa fa-chrome"></i>
                            <span class="nav-label" style="font-size: 14px;">最近浏览</span>
                        </a>
                    </li>
                    <li>
                        <a href="#collapseFavTree" data-toggle="collapse">
                            <i class="fa fa-heart"></i>
                            <span class="nav-label" style="font-size: 14px;">我的收藏</span>
                            <span class="fa arrow"></span>
                        </a>
                        <div id="collapseFavTree" class="panel-collapse collapse" style="background-color:#ffffff;">
                            <div class="panel-body" style="padding: 5px;">
                                <div class="form-group">
                                    @*<input id="txtTarget" type="text" class="form-control" style="font-size: 12px;">
                                        <span class="help-block m-b-none" style="font-size: 11px;"></span>*@
                                    <a class="btn" id="favCatalog_add_sibling" title="新增同级" data-toggle="tooltip"><i class="fa fa-plus fa-fw"></i>新增同级</a>
                                    <a class="btn" id="favCatalog_add_child" title="新增子级" data-toggle="tooltip"><i class="fa fa-plus-square fa-fw"></i>新增子级</a>
                                    <a class="btn disabled" id="favCatalog_rename" title="重命名" data-toggle="tooltip"><i class="fa fa-pencil fa-fw"></i>重命名</a>
                                    <a class="btn disabled" id="favCatalog_delete"><i class="fa fa-trash-o fa-fw"></i>删除</a>
                                    <input class="form-control" type="text" id="favSelect_search" placeholder="查找收藏夹" />
                                </div>
                                <div class="form-group">
                                    <div id="favCatalog_jsTree"></div>
                                </div>
                            </div>

                        </div>
                    </li>
                    <li>
                        <a href="javascript:switchtab(3)">
                            <i class="fa fa-download"></i>
                            <span class="nav-label" style="font-size: 14px;">我的下载</span>
                        </a>
                    </li>
                    <li>
                        <a href="#collapseTheme" data-toggle="collapse">
                            <i class="fa fa-gear"></i>
                            <span class="nav-label" style="font-size: 14px;">主题管理</span>
                            <span class="fa arrow"></span>
                        </a>
                        <div id="collapseTheme" class="panel-collapse collapse" style="background-color:#ffffff;">
                            <div class="panel-body" style="padding: 5px;">
                                <div class="form-group" style="margin-bottom:0;">
                                    <a href="javascript:switchtab(4)">

                                        <span class="nav-label" style="font-size: 14px;margin-left:20px;line-height:35px;color:#337ab7;">投影样式设定</span>
                                    </a>
                                </div>
                                <div class="form-group" style="margin-bottom:0;">
                                    <a href="javascript:switchtab(5)">

                                        <span class="nav-label" style="font-size: 14px;margin-left:20px;line-height:35px;color:#337ab7;">开发曲线模板</span>
                                    </a>
                                </div>
                                <div class="form-group" style="margin-bottom:0;">
                                    <a href="javascript:switchtab(6)">

                                        <span class="nav-label" style="font-size: 14px;margin-left:20px;line-height:35px;color:#337ab7;">图数联动设定</span>
                                    </a>
                                </div>
                            </div>
                        </div>
                    </li>
                    <li>
                        <a href="#collapseGMFile" data-toggle="collapse">
                            <i class="fa fa-asterisk"></i>
                            <span class="nav-label" style="font-size: 14px;">图件管理</span>
                            <span class="fa arrow"></span>
                        </a>
                        <div id="collapseGMFile" class="panel-collapse collapse" style="background-color:#ffffff;">
                            <div class="panel-body" style="padding: 5px;">
                                <div class="form-group" style="margin-bottom:0;">
                                    <a href="javascript:switchtab(7)">
                                        <span class="nav-label" style="font-size: 14px;margin-left:20px;line-height:35px;color:#337ab7;">图件提交</span>
                                    </a>
                                </div>
                                <div class="form-group" style="margin-bottom:0;">
                                    <a href="javascript:switchtab(8)">
                                        <span class="nav-label" style="font-size: 14px;margin-left:20px;line-height:35px;color:#337ab7;">图件审核</span>
