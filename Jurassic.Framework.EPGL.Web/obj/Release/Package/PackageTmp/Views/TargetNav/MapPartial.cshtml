
<div id="panelMap" style="height: 100%; width: 100%;">
    
    <div id="gis" style="width: 100%; height: 100%;">

    </div>
    <div class="ol-unselectable ol-control" id="tool_div" style="top: calc(100% - 280px); left: calc(100% - 40px);">
        <a role="button" title="导入">
            <input id="file-input" name="file-input" type="file" class="file-loading" accept=".csv, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
        </a>
        
        <button id="ogis_export" title="导出" class="tool-btn"><i class="fa fa-sign-out fa-fw"></i></button>
        <button id="ogis_line" title="长度测量" class="tool-btn"><i class="fa fa-arrows-h fa-fw"></i></button>
        <button id="ogis_area" title="面积测量" class="tool-btn"><i class="fa fa-lemon-o fa-fw"></i></button>
        <button id="ogis_changestyle" title="图层样式" class="tool-btn" data-toggle="modal" data-target="#changeStyleModal"><i class="fa fa-gear fa-fw"></i></button>
        <button id="ogis_reset" title="还原" class="tool-btn"><i class="fa fa-undo fa-fw"></i></button>
        <button id="ogis_zoomin" title="放大" class="tool-btn" type="button"><i class="fa fa-plus fa-fw"></i></button>
        <button id="ogis_zoomout" title="缩小" class="tool-btn" type="button"><i class="fa fa-minus fa-fw"></i></button>
    </div>
    <div id="mouse-position" class="mouse-position-wrapper">
        <div class="custom-mouse-position"></div>
    </div>

    <div class="modal fade" tabindex="-1" role="dialog" id="changeStyleModal" aria-labelledby="changeStyleModalLabel">
        <div class="modal-dialog modal-lg" role="document">
            <div class="modal-content">
                <div class="modal-body" id="styleModal_content">
                    <div class="row">
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <div style="padding:10px;text-align:center;">目标类型名称</div>
                        </div>
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <div style="padding:10px;text-align:center;">图层填充颜色</div>
                        </div>
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <div style="padding:10px;text-align:center;">边界颜色</div>
                        </div>
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <div style="padding:10px;text-align:center;">文本颜色</div>
                        </div>
                    </div>                  
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" id="changeStyle_OK">确定</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>                   
                </div>
            </div><!-- /.modal-content -->
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->
</div>



